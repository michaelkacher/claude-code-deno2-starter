{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false,
  "deno.path": "deno",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.exclude": {
    "**/.fresh": true,
    "**/dist": true,
    "**/coverage": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/.fresh": true,
    "**/coverage": true,
    "**/deno.lock": true
  }
}
