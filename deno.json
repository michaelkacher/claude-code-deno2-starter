{
  "name": "@your-org/web-project-starter",
  "version": "0.1.0",
  "exports": "./main.ts",
  "nodeModulesDir": "manual",
  "lock": false,
  "tasks": {
    "dev": "deno run -A --unstable-kv npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "deploy": "deployctl deploy --project=your-project-name --prod --entrypoint=main.ts",
    "test": "DENO_ENV=test deno test --allow-read --allow-env --allow-net --allow-write --unstable-kv --no-check=remote",
    "test:watch": "DENO_ENV=test deno test --allow-read --allow-env --allow-net --allow-write --unstable-kv --watch",
    "test:coverage": "DENO_ENV=test deno test --allow-read --allow-env --allow-net --allow-write --unstable-kv --coverage=coverage",
    "coverage": "deno coverage coverage --lcov --output=coverage.lcov",
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "typecheck": "deno check routes/**/*.ts routes/**/*.tsx islands/**/*.tsx main.ts",
    "lint": "deno lint",
    "lint:fix": "deno lint --fix",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "validate-imports": "deno run --allow-read scripts/validate-import-paths.ts",
    "kv:seed": "deno run --allow-read --allow-write --allow-env scripts/seed-local-kv.ts",
    "kv:reset": "deno run --allow-read --allow-write scripts/reset-local-kv.ts",
    "kv:inspect": "deno run --allow-read --allow-write --allow-env scripts/inspect-local-kv.ts",
    "kv:seed-user": "deno run --unstable-kv --allow-read --allow-write --allow-env scripts/seed-test-user.ts",
    "kill-ports": "deno run --allow-run scripts/kill-ports.ts",
    "reset-rate-limits": "deno run --unstable-kv --allow-read --allow-write --allow-env scripts/reset-rate-limits.ts",
    "users:list": "deno run --unstable-kv --allow-read --allow-write --allow-env scripts/list-users.ts",
    "users:make-admin": "deno run --unstable-kv --allow-read --allow-write --allow-env scripts/make-admin.ts",
    "test-notifications": "deno run --unstable-kv --allow-read --allow-write --allow-env scripts/quick-test-notifications.ts",
    "update": "deno run -A -r jsr:@fresh/update"
  },
  "deploy": {
    "project": "your-project-name",
    "entrypoint": "main.ts",
    "exclude": [
      "dist",
      "coverage",
      ".fresh",
      "_fresh",
      "tests",
      ".git"
    ]
  },
  "imports": {
    "@/": "./",
    "fresh": "jsr:@fresh/core@^2.1.4",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.7",
    "vite": "npm:vite@^7.1.3",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "preact": "npm:preact@^10.27.2",
    "preact/debug": "npm:preact@^10.27.2/debug",
    "preact/jsx-runtime": "npm:preact@^10.27.2/jsx-runtime",
    "preact/jsx-dev-runtime": "npm:preact@^10.27.2/jsx-dev-runtime",
    "preact/hooks": "npm:preact@^10.27.2/hooks",
    "@preact/signals": "npm:@preact/signals@^2.5.0",
    "@preact/signals-core": "npm:@preact/signals-core@^1.10.2",
    "$std/": "https://deno.land/std@0.216.0/",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0",
    "tailwindcss": "npm:tailwindcss@^4.0.0",
    "tailwindcss/": "npm:tailwindcss@^4.0.0/",
    "tailwindcss/plugin": "npm:tailwindcss@^4.0.0/plugin.js",
    "bcryptjs": "npm:bcryptjs@^2.4.3",
    "zod": "npm:zod@^3.24.1",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.0.0",
    "@aws-sdk/lib-storage": "npm:@aws-sdk/lib-storage@^3.0.0",
    "@aws-sdk/s3-request-presigner": "npm:@aws-sdk/s3-request-presigner@^3.0.0"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": ["vite/client"],
    "lib": ["deno.window", "dom", "dom.iterable", "deno.unstable"],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "useUnknownInCatchVariables": false
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"],
      "include": [
        "ban-untagged-todo",
        "no-throw-literal",
        "no-const-assign",
        "no-debugger",
        "no-duplicate-case",
        "no-empty",
        "no-empty-character-class",
        "no-empty-pattern",
        "no-ex-assign",
        "no-explicit-any",
        "no-fallthrough",
        "no-invalid-regexp",
        "no-irregular-whitespace",
        "no-prototype-builtins",
        "no-self-assign",
        "no-sparse-arrays",
        "no-unreachable",
        "no-unsafe-finally",
        "use-isnan",
        "valid-typeof",
        "jsx-button-has-type"
      ],
      "exclude": [
        "no-unused-vars",
        "require-await",
        "no-await-in-loop",
        "no-window"
      ]
    },
    "exclude": [
      "dist",
      "coverage",
      "**/_fresh/*",
      "**/*.template.ts",
      "**/*.template.tsx"
    ]
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve",
    "exclude": [
      "dist",
      "coverage"
    ]
  },
  "test": {
    "include": [
      "**/*_test.ts",
      "**/*.test.ts",
      "tests/**/*.test.ts"
    ],
    "exclude": [
      "dist",
      "coverage"
    ]
  },
  "exclude": [
    "dist",
    "coverage",
    "**/_fresh/*"
  ]
}
